{
  "name": "anfischer/laravel-foundation",
  "type": "library",
  "description": "Laravel Foundation - a framework inspired by Lucid Foundation",
  "keywords": [
    "anfischer",
    "laravel-foundation"
  ],
  "homepage": "https://github.com/anfischer/laravel-foundation",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Fischer",
      "email": "kontakt@season.dk",
      "homepage": "https://season.dk",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": ">=5.4.3",
    "mockery/mockery": "^1.1",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Anfischer\\Foundation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Anfischer\\Foundation\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=ruleset.xml --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=ruleset.xml --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
